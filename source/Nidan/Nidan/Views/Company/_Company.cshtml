@model Nidan.Models.CompanyViewModel
<div class="form-control-required-example"> = Required</div>
<br />
<br />
<div class="form-group">
    @Html.LabelFor(model => model.Company.Name, new { @class = "control-label col-md-4 col-sm-6 col-xs-12" })
    <div class="col-md-12 col-sm-8 col-xs-12 name">
        @Html.TextBoxFor(model => model.Company.Name, new { @class = "form-control has-feedback-left" })
        <span class="fa fa-user form-control-feedback left" aria-hidden="true"></span>
        @Html.ValidationMessageFor(model => model.Company.Name, "", new { @class = "help-block" })
    </div>
</div>

